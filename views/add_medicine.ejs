    <script>
        function generateMedicineForms() {
            const n = document.getElementById("number_of_medicines").value;
            const container = document.getElementById("medicine_forms");
            container.innerHTML = "";
            for (let i = 0; i < n; i++) {
                const formGroup = `
                    <div>
                        <h3>Medicine ${i + 1}</h3>
                        <label for="medicine_name_${i}">Medicine Name:</label>
                        <input type="text" id="medicine_name_${i}" name="medicine_name_${i}" required><br>
                        <label for="dosage_${i}">Dosage:</label>
                        <input type="text" id="dosage_${i}" name="dosage_${i}" required><br>
                        <label for="frequency_${i}">Frequency (seconds):</label>
                        <input type="number" id="frequency_${i}" name="frequency_${i}" required><br>
                        <label for="duration_type_${i}">Duration Type:</label>
                        <select id="duration_type_${i}" name="duration_type_${i}" onchange="toggleDurationInput(this, ${i})" required>
                            <option value="" disabled selected>Select Duration</option>
                            <option value="daily">Daily</option>
                            <option value="days">For Some Days</option>
                        </select><br>
                        <div id="duration_days_input_${i}" style="display:none;">
                            <label for="duration_${i}">Duration (days):</label>
                            <input type="number" id="duration_${i}" name="duration_${i}"><br>
                        </div>
                        <label for="start_date_${i}">Start Date:</label>
                        <input type="datetime-local" id="start_date_${i}" name="start_date_${i}" required><br>
                        <label for="recipient_email_${i}">Recipient's Email:</label>
                        <input type="email" id="recipient_email_${i}" name="recipient_email_${i}" required><br>
                    </div>
                `;
                container.innerHTML += formGroup;
            }
        }

        function toggleDurationInput(select, index) {
            const durationInputDiv = document.getElementById(`duration_days_input_${index}`);
            if (select.value === 'days') {
                durationInputDiv.style.display = 'block';
            } else {
                durationInputDiv.style.display = 'none';
            }
        }
    </script>
